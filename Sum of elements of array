#include <stdio.h>
int main() {
	//code
	int test,k=0;
	scanf("%d",&test);
	while(k<test){ // for test cases
	    k++; 
	    int i,n;
	    scanf("%d",&n); // size of array
	    int a[n],sum=0;
	    for(i=0;i<n;i++) // loop to read elements
	    scanf("%d",&a[i]);
	    for(i=0;i<n;i++) 
	    sum += a[i]; // adding all the elements
	    printf("%d\n",sum);
	}
	return 0;
}
